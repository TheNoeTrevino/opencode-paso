{
  "name": "opencode-paso",
  "version": "1.0.0",
  "description": "OpenCode plugin for paso workflow context injection",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build index.ts --outdir dist --target node",
    "prepublishOnly": "bun run build",
    "test": "bun test"
  },
  "keywords": [
    "opencode",
    "paso",
    "kanban",
    "workflow",
    "plugin",
    "ai"
  ],
  "author": "TheNoeTrevino",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thenoetrevino/opencode-paso.git"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": ">=0.15.0",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^0.15.0",
    "@types/bun": "^1.3.1",
    "typescript": "^5.9.3"
  }
}
